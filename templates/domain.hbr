package {{prop.entityPackageName}};
{{#each table.importList}}
import {{this}}
{{/each}}

import {{prop.baseEntityPackageName}}.BaseEntity;
import com.ftrybe.tourismend.core.validator.groups.DefaultGroup;
import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

import javax.validation.constraints.NotBlank;
import javax.validation.constraints.NotNull;
import javax.validation.constraints.Size;

/**
 *{{table.actualTableName}}
 *{{table.remarks}}
 */
@Data
@AllArgsConstructor
@NoArgsConstructor
@ApiModel(value = "{{table.remarks}}",description = "{{table.remarks}}")
public class {{table.tableName}} extends BaseEntity{
    {{#each table.allColumns}}
	<#list  table.allColumns as column>
		<#if column.columnNameLower != "id"  && column.columnNameLower != "createId" &&
        column.columnNameLower != "createTime" && column.columnNameLower != "updateId" && column.columnNameLower != "updateTime" &&
        column.columnNameLower != "deptKey" && column.columnNameLower != "areaKey" && column.columnNameLower != "delFlag">
