    *{{column.remarks}}
    */
    @ApiModelProperty(value = "{{column.remarks}}，长度为：{{column.columnSize}} <#if column.isNullable == "NO">，不可为空。</#if>",position = {{column_index+1}})
    <#if column.isNullable == "NO">
        <#if (column.javaTypeShortName=="String")>
    @NotBlank(groups = {DefaultGroup.class}})
        <#else>
    @NotNull(groups = {DefaultGroup.class}})
        </#if>
    </#if>
    <#if (column.javaTypeShortName=="String")>
    @Size(max = {{column.columnSize}}, groups = {DefaultGroup.class}})
    </#if>
    private {{column.javaTypeShortName}} {{column.columnNameLower}};

	</#if>
	{{/each}}
}